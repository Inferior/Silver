<product name="silver Server" id="org.asd.silver.server.product" application="org.asd.silver.server.app" useFeatures="false" includeLaunchers="true">

   <configIni use="default">
      <linux>/org.asd.silver.server/products/production/config.ini</linux>
      <macosx>/org.asd.silver.server/products/production/config.ini</macosx>
      <win32>/org.asd.silver.server/products/production/config.ini</win32>
   </configIni>

   <launcherArgs>
      <programArgsLin>-os ${target.os} -ws ${target.ws} -arch ${target.arch} -nl ${target.nl}
      </programArgsLin>
      <programArgsMac>-os ${target.os}  -arch ${target.arch} -nl ${target.nl}
      </programArgsMac>
      <programArgsWin>-os ${target.os} -ws ${target.ws} -arch ${target.arch} -nl ${target.nl}
      </programArgsWin>
      <vmArgs>-Xms64m
-Xmx512m
      </vmArgs>
   </launcherArgs>

   <launcher>
      <solaris/>
      <win useIco="false">
         <bmp/>
      </win>
   </launcher>

   <vm>
   </vm>

